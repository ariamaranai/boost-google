{
  "manifest_version": 3,
  "name": "boost-google",
  "version": "0",
  "description": "Boost google search",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "declarativeNetRequest"
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "0",
      "path": "0.json",
      "enabled": true
    }]
  },
  "content_scripts": [{
    "css": ["main.css"],
    "js": ["main.js"],
    "matches": ["https://www.google.com/search*"],
    "world": "MAIN",
    "run_at": "document_start"
  }]
}